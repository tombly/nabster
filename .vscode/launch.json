{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Report: Performance",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "report",
                "performance",
                "--config-file",
                "config.local.json"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Report: Planning",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "report",
                "planning",
                "--config-file",
                "config.local.json"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Report: Spend",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "report",
                "spend",
                "--category-name",
                "<CATEGORY NAME>",
                "--month",
                "2024-12",
                "--config-file",
                "config.local.json"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Chat: Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "name": "Chat: Direct",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "chat",
                "direct",
                "--message",
                "What is my Checking balance?"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Chat: Local",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "chat",
                "func",
                "--message",
                "What is my Checking balance?",
                "--config-file",
                "config.local.json"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Chat: Azure",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "chat",
                "func",
                "--message",
                "What is my Checking balance?"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}