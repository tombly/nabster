{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Planning Report",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Nabster.dll",
            "args":["planning", "<BUDGET NAME>", "<ACCESS TOKEN>", "<INPUT FILE PATH>"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Performance Report",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Nabster.dll",
            "args":["performance", "<BUDGET NAME>", "<ACCESS TOKEN>", "<INPUT FILE PATH>"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Spend Report",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Nabster.dll",
            "args":["spend", "<BUDGET NAME>", "<ACCESS TOKEN>", "<CATEGORY NAME>"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}