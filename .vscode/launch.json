{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Local:Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "name": "Local:Activity",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
            "args": [
                "activity",
                "--category-name",
                "<SET THIS>",
                "--phone-number",
                "<SET THIS>",
                "--config-file",
                "config.local.json"
            ],
            "cwd": "${workspaceFolder}/Nabster.Cli",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Local:Funded",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/Nabster.Cli/bin/Debug/net8.0/Nabster.Cli.dll",
